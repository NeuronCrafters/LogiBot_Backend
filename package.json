{
  "name": "logibot_backend",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "swagger": "ts-node src/config/swagger/swaggerConfig.ts",
    "docker:dev": "docker compose -f docker-compose.dev.yml up -d --build",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "db:seed": "npx ts-node src/mock/mock.ts",
    "docker:down:clean": "docker compose -f docker-compose.dev.yml down -v",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d --build",
    "wait": "node -e \"\"",
    "db:restore:internal": "docker exec logibot_mongodb_dev mongorestore --username chatsael --password chatsael_password --authenticationDatabase admin --db logibot_db --drop /data/backup_files",

    "fresh-start": "npm run docker:down:clean && npm run docker:up && npm run wait && npm run db:restore:internal",
    "dev:setup": "npm-run-all -s docker:dev wait db:seed dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.1",
    "express-useragent": "^1.0.15",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@faker-js/faker": "^10.1.0",
